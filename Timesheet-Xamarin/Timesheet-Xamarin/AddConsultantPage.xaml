<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Timesheet_Xamarin.AddConsultantPage">
    <ContentPage.Content>
        <StackLayout>
        <Entry
            x:Name="Email"
            Placeholder="Enter Email"
            Text=""
        />
            <Button
                x:Name="AddConsultant"
                Text="Add Consultant"
                Padding="3"
                ScaleX="0.75"
                ScaleY="0.75"
                Clicked=""
                />

            <Button
                x:Name="Back"
                Text="Back"
                Padding="3"
                ScaleX="0.75"
                ScaleY="0.75"
                Clicked="CancelButton_Clicked"
                />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>