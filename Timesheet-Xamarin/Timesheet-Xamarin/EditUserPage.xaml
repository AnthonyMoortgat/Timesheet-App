<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Timesheet_Xamarin.EditUserPage"
             xmlns:ia="clr-namespace:IntelliAbb.Xamarin.Controls;assembly=IntelliAbb.Xamarin.Controls">
    <ContentPage.Content>
        <StackLayout>
            <Label 
                x:Name="Email" 
                FontSize="15"
                VerticalOptions="Center"
                />
            <Label 
                x:Name="Name" 
                FontSize="10"
                VerticalOptions="Center"
                />
            <StackLayout x:Name="RolesList" VerticalOptions="Center">

            </StackLayout>
            <Button 
                Text="Cancel" 
                x:Name="Cancel"
                Clicked="ClickCancel"
                Scale="0.75"
                />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>